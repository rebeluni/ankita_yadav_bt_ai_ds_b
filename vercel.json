{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "matchmaking/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/socket.io/",
      "dest": "matchmaking/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ]
}
